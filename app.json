{
  "expo": {
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#8d2fc7"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Filmeo to access your photos."
        }
      ]
    ],
    "name": "filmeo",
    "slug": "filmeo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8d2fc7",
      "dark": {
        "image": "./assets/splash1.png",
        "resizeMode": "cover",
        "backgroundColor": "#8d2fc7d6"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.adebunmy.filmeo",
      "googleServicesFile": "./android/app/google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0d22ae9e-0ea0-4bfd-be36-99fc0d8e4caa"
      }
    },
    "scheme": "filmeo"
  }
}
